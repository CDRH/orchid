  <!-- Scripts -->
  <%= javascript_include_tag 'application' %>
  <% if @ext_js.present? || @inline_js.present? %>
  <!-- Conditional -->
  <% end %>
  <% if @ext_js.present?
    Array.wrap(@ext_js).each do |js| %>
  <%= javascript_include_tag js %>
    <% end
  end %>
  <% if @inline_js.present?
    Array.wrap(@inline_js).each do |js| %>
  <script>
<%= js.html_safe %>
  </script>
    <% end
  end %>
