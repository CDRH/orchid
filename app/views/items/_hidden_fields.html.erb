<!-- include existing parameters-->
<% if params["f"].present? %>
  <% params["f"].each do |f| %>
    <%= hidden_field_tag "f[]", f %>
  <% end %>
<% end %>

<% if params["date_from"].present? %>
  <% params["date_from"].each do |d| %>
    <%= hidden_field_tag("date_from[]", d) %>
  <% end %>
<% end %>
<% if params["date_to"].present? %>
  <% params["date_to"].each do |d| %>
    <%= hidden_field_tag("date_to[]", d) %>
  <% end %>
<% end %>
